'''
A simple example to run CISD calculation.
'''

import pyscf
import pyscf.scf
import pyscf.ci

mol = pyscf.M(
    atom = 'H 0 0 0; H 0 0 1.3',
    basis = 'ccpvdz',
    verbose=4)

mf = pyscf.scf.RHF(mol)
mf.run()
cc = pyscf.ci.CISD(mf)
cc.run()
print('RCISD correlation energy', cc.e_corr)

C0, C1 ,C2 = cc.cisdvec_to_amplitudes(cc.ci)
print(C0.shape)
print(C1.shape)
print(C2.shape)


print(C0)

